#include <stdio.h>
struct Book {
    int id;
    char title[50];
    float price;
};

int main() {
    int n, i;
    struct Book b[10];

    printf("Enter number of books: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        scanf("%d %s %f", &b[i].id, b[i].title, &b[i].price);
    }

    for(i = 0; i < n; i++) {
        printf("\nID: %d  Title: %s  Price: %.2f", b[i].id, b[i].title, b[i].price);
    }
    return 0;
}
