#include <stdio.h>
int main() {
    int i,n,found=0;
    float price[20], key;

    printf("Enter number of items: ");
    scanf("%d",&n);

    for(i=0;i<n;i++){
        printf("Enter price of item %d: ", i+1);
        scanf("%f",&price[i]);
    }

    printf("Enter price to search: ");
    scanf("%f",&key);

    for(i=0;i<n;i++){
        if(price[i]==key){
            found=1; break;
        }
    }

    if(found)
        printf("Item price found");
    else
        printf("Item price not found");

    return 0;
}
