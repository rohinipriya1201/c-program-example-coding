#include <stdio.h>
int main() {
    int n,i,marks[50],top;

    printf("Enter number of students: ");
    scanf("%d",&n);

    for(i=0;i<n;i++){
        scanf("%d",&marks[i]);
    }

    top = marks[0];
    for(i=1;i<n;i++){
        if(marks[i] > top)
            top = marks[i];
    }

    printf("Topper marks = %d", top);
    return 0;
}
