#include <stdio.h>
int main() {
    int temp[7], i, max, min;

    for(i=0;i<7;i++){
        printf("Enter temperature for day %d: ", i+1);
        scanf("%d",&temp[i]);
    }

    max = min = temp[0];
    for(i=1;i<7;i++){
        if(temp[i] > max) max = temp[i];
        if(temp[i] < min) min = temp[i];
    }

    printf("Highest = %d\nLowest = %d", max, min);
    return 0;
}
